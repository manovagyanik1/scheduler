{
  "manifest_version": 2,

  "name": "fb post scheduler",
  "description": "Will post on lolmenow page",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "contextMenus",
   "tabs"
   ],
   "background": {
       "page": "background.html"
   }
}